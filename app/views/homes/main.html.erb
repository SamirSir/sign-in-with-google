<h2>Yap You are logged in!</h2>
<hr>

<h2>
	<% if current_user.full_name != nil %>
		Name: <%= current_user.full_name %><br>
		<%= image_tag(current_user.avatar_url, style:'max-width:100px, max-height:100px; ') %><br>
	<% else %>
		Name: <%= current_user.username %><br>
	<% end %>
	Id: <%= current_user.id %><br>
	Email: <%= current_user.email %><br>
	
</h2>
<hr>
<%= link_to "Sign Out", destroy_user_session_path , method: :delete %>
